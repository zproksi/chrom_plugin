{
    "manifest_version": 3,
    "name": "overlays cleaner",
    "version": "1.0.1",
    "description": "use if you tired about overlays at your sites",
    "action":{
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      },
      "default_popup": "popup.html"
    },
    "permissions":["activeTab", "storage"],
    "content_scripts":[{
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_idle"
    }],
    "background": {
      "service_worker": "background.js"
    }
}